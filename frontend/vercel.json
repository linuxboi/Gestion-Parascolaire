{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }


  
  